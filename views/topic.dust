{>bootstrap_header/}
<div class="com-ibm-team-jazzhub-Docs content fluid-content">
	<div id="com-ibm-team-jazzhub-Announcement" class="com-ibm-team-jazzhub-Announcement" style="display: none"></div>
		<div class="container-fluid topic">
			<div class="row" id="topic-header">
				<div class="col-md-12">
					<div class="row">
						<div class="col-md-2 col-sm-4 col-xs-5 vcenter">
							<div class="topic-img">
								<img src="images/{imgicon}.png" />
							</div>
						</div>
						<div class="col-md-6 vcenter topic-header-name">
							<span>{topicname}</span>
						</div>
					</div>
					<div class="clearfix"></div>
				</div>
			</div>
			<ol class="breadcrumb">
				<li><a href="/docs">{sectionname}</a></li>
				<li><a href="/docs#topics">All Topics</a></li>
				<li class="current">{topicname}</li>
			</ol>
			<div class="row">
				<div class="col-md-12 topic pbl">
						{markdown|s}
				</div>
			</div>
		</div>
	</div>
</div>
{>footer/}

